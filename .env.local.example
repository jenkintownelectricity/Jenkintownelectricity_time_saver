# ==================================================
# SUPABASE CONFIGURATION (Database & Authentication)
# ==================================================
# Get your credentials at https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ==================================================
# VAPI CONFIGURATION (Voice AI)
# ==================================================
# Get your keys at https://vapi.ai
NEXT_PUBLIC_VAPI_KEY=your_vapi_public_key_here
VAPI_ASSISTANT_ID=your_vapi_assistant_id_here

# ==================================================
# ANTHROPIC CONFIGURATION (Photo Analysis)
# ==================================================
# Get your key at https://console.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# ==================================================
# OWNER/ADMIN DEFAULT KEYS (Optional)
# ==================================================
# These will be used when users haven't configured their own API keys
NEXT_PUBLIC_DEFAULT_VAPI_KEY=your_default_vapi_key_here
NEXT_PUBLIC_DEFAULT_VAPI_ASSISTANT_ID=your_default_assistant_id_here
NEXT_PUBLIC_DEFAULT_ANTHROPIC_KEY=your_default_anthropic_key_here

# ==================================================
# NOTE: Do NOT set NODE_ENV manually!
# ==================================================
# Next.js automatically sets NODE_ENV based on the command:
# - npm run dev → development
# - npm run build/start → production
# Setting it manually causes inconsistencies and warnings.
