# VAPI Configuration (for Voice AI)
# Get your keys at https://vapi.ai
NEXT_PUBLIC_VAPI_KEY=your_vapi_public_key_here
VAPI_ASSISTANT_ID=your_vapi_assistant_id_here

# Anthropic Configuration (for Photo Analysis)
# Get your key at https://console.anthropic.com
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Owner/Admin Default Keys (Optional - only if you want to provide fallback keys for users)
# These will be used when users haven't configured their own API keys
NEXT_PUBLIC_DEFAULT_VAPI_KEY=your_default_vapi_key_here
NEXT_PUBLIC_DEFAULT_VAPI_ASSISTANT_ID=your_default_assistant_id_here
NEXT_PUBLIC_DEFAULT_ANTHROPIC_KEY=your_default_anthropic_key_here

# Optional: Environment
NODE_ENV=production
